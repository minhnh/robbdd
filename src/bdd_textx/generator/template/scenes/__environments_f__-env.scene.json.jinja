{
    "@context": [
        "{{ secoro_mm }}/acceptance-criteria/bdd/environment.json",
        "{{ secoro_mm }}/acceptance-criteria/bdd/bdd.json",
        {
            "{{ namespaces.environments.name }}": "{{ namespaces.environments.uri }}",
            "{{ namespaces.scene_env.name }}": "{{ namespaces.scene_env.uri }}"
        }
    ],
    "@graph": [
        { "@id": "{{ namespaces.scene_env.name }}:{{ scene.model.name }}", "@type": "bdd:Scene" },
        {
            "@id": "{{ namespaces.scene_env.name }}:{{ scene.model.name }}-{{ namespaces.environments.name }}-objects", "@type": "bdd:SceneHasObjects",
            "of-scene": "{{ namespaces.scene_env.name }}:{{ scene.model.name }}",
            "has-object": [
                {%+ for object in scene.model.objects %}"{{ namespaces.environments.name }}:obj-{{ object.name }}", {% endfor +%}
                {%+ for workspace in scene.model.workspaces %}"{{ namespaces.environments.name }}:obj-{{ workspace.name }}"{% if not loop.last %}, {% endif %}{% endfor +%}
            ]
        },
        {
            "@id": "{{ namespaces.scene_env.name }}:{{ scene.model.name }}-{{ namespaces.environments.name }}-workspaces", "@type": "bdd:SceneHasWorkspaces",
            "of-scene": "{{ namespaces.scene_env.name }}:{{ scene.model.name }}",
            "has-workspace": [
                {%+ for workspace in scene.model.workspaces %}"{{ namespaces.environments.name }}:ws-{{ workspace.name }}"{% if not loop.last %}, {% endif %}{% endfor +%}
            ]
        }
    ]
}
{
    "@context": [
        "{{ secoro_mm }}/acceptance-criteria/bdd/environment.json",
        {
            "{{ namespaces.environments.name }}": "{{ namespaces.environments.uri }}"
        }
    ],
    "@graph": [
        {% for object in scene.model.objects %}
        { "@id": "{{ namespaces.environments.name }}:obj-{{ object.name }}", "@type": "env:Object" },
        {% endfor -%}
        {% for workspace in scene.model.workspaces %}
        { "@id": "{{ namespaces.environments.name }}:obj-{{ workspace.name }}", "@type": "env:Object" },
        {% endfor %}

        {% for workspace in scene.model.workspaces %}
        { "@id": "{{ namespaces.environments.name }}:ws-{{ workspace.name }}", "@type": "env:Workspace" },
        {% endfor %}

        {% for workspace in scene.model.workspaces %}
        {
            "@id": "{{ namespaces.environments.name }}:{{ workspace.name }}-object", "@type": "env:WorkspaceHasObject",
            "of-workspace": "{{ namespaces.environments.name }}:ws-{{ workspace.name }}",
            "has-object": "{{ namespaces.environments.name }}:obj-{{ workspace.name }}"
        },
        {% endfor %}

        { "@id": "{{ namespaces.environments.name }}:{{ namespaces.environments.name }}", "@type": "env:Workspace" },
        {
            "@id": "{{ namespaces.environments.name }}:{{ namespaces.environments.name }}-workspaces", "@type": "env:WorkspaceHasWorkspace",
            "of-workspace": "{{ namespaces.environments.name }}:ws-{{ namespaces.environments.name }}",
            "has-workspace": [
                {%+ for workspace in scene.model.workspaces %}"{{ namespaces.environments.name }}:ws-{{ workspace.name }}"{% if not loop.last %}, {% endif %}{% endfor +%}
            ]
        }
    ]
}
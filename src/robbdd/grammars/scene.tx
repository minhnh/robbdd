import base

Model:

namespaces=NamespaceDeclare*

model=SceneModel
;

SceneModel:
'scene' '(' 'ns' '=' ns=[NamespaceDeclare|FQN] ')' name=ID '{'
(
objects+=Object
|
workspaces+=Workspace
|
agents+=Agent
)*
'}'
;

Object: 'object' '(' 'ns' '=' ns=[NamespaceDeclare|FQN] ')' name=ID ';' ;
Workspace:
'workspace' '(' 'ns' '=' ns=[NamespaceDeclare|FQN] ')' name=ID
(
';'
|
'{'
(
objects+=Object
|
workspaces+=Workspace
)*
'}'
)
;
Agent: 'agent' '(' 'ns' '=' ns=[NamespaceDeclare|FQN] ')' name=ID ';' ;
